#include<iostream>

int main()
{
    for (int i = 1; i <=8; i++)
    {
        for (int j = 1; j <=54 ; j++)
        {
            if ((i==1 && j<=6) || j==3 || (i==8 && j<=6) ||(i==8 && j==13)
            ||(i==7 && j==12)||(i==6 && j==11) || (i==5 && j==10) || 
            (i==4 && j==10)
            ||(i==3 && j==10) || (i==2 && j==12) || (i==3 && j==13)
             || (i==2 && j==14)
            ||(i==3 && j==16) || (i==4 && j==16) ||(i==5 && j==16)||
            (i==6 && j==15)
            ||(i==7 && j==14)||(i==8 && j==13) || (i==1 && j==20)||(j==22)||
            (i==1 && j>=22 && j<=26)
            ||(i==4 && j>=22 && j<=26) ||(j==26 && i<=4) || 
            (i==1 && j>=28 && j<=35) || (j==35)
            ||(i==8 && j>=28 && j<=35) || (j==30) || (j==38) 
            ||(i==1 && j>=39 && j<=43)
            ||(i==5 && j>=39 && j<=43) || (i==8 && j>=39 && j<=43) 
            ||(j==47)||(j==54)||
            (i==8 && j>=47 && j<=54)
            
            )
            
            {
               std::cout<<"*";
            }
            else{
                std::cout<<" ";
            }
            
        }
       std::cout<<std::endl; 
    }
    

    return 0;
}
